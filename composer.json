{
    "name": "patrickteunissen/coinbase",
    "description": "Coinabase Api",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Teunissen",
            "email": "p.teunissen18@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefere-stable": true,
    "require": {
        "coinbase/coinbase": "^2.5",
        "monolog/monolog": "^2.0",
        "predis/predis": "^2.0",
        "symfony/console": "^4.1",
        "symfony/debug": "^4.1",
        "symfony/dotenv": "^4.1",
        "symfony/http-foundation": "^4.1",
        "symfony/polyfill-php71": "^1.6",
        "symfony/var-dumper": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
